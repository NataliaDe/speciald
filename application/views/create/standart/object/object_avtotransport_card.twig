<div class="form-row create-form-row">

{%set i=0%}
    <div class="form-group col-md-2">
        <label for="avtotransport_vid" >Вид</label>
        <select class="form-control select2-single-face-belong" name="avtotransport[{{i}}][avto_vid]" id="avto_vid"  >
            <option></option>
            {% for vid in avtotransport_vid %}
                <option value="{{ vid.id }}" {%if (is_edit_dones is defined and is_edit_dones == 1 and dones.object is defined and dones.object.avto_vid == vid.id)%}selected{%endif%} >{{ vid.name }}</option>
            {% endfor %}
        </select>
    </div>


    <div class="form-group col-md-1">

        <label for="avto_year">Год выпуска</label>
        <input type="number" id="avto_year"  max="{{now|date("Y")}}" class="form-control"  placeholder="" name="avtotransport[{{i}}][avto_year]"  value="{%if is_edit_dones is defined and is_edit_dones == 1 and dones is defined and dones.object.avto_year is defined and dones.object.avto_year is not empty %}{{dones.object.avto_year|trim}}{%endif%}">

    </div>


    <div class="form-group col-md-2">

        <label for="avto_type_fuel">Тип топлива</label>
        <input type="text" id="avto_type_fuel" class="form-control"  placeholder="" name="avtotransport[{{i}}][avto_type_fuel]"  value="{%if is_edit_dones is defined and is_edit_dones == 1 and dones is defined and dones.object.avto_type_fuel is defined and dones.object.avto_type_fuel is not empty %}{{dones.object.avto_type_fuel|trim}}{%endif%}">

    </div>


    <div class="form-group col-md-2">

        <label for="register_sign">Регистрационный знак</label>
        <input type="text" id="avto_register_sign" class="form-control"  placeholder="" name="avtotransport[{{i}}][avto_register_sign]"  value="{%if is_edit_dones is defined and is_edit_dones == 1 and dones is defined and dones.object.avto_register_sign is defined and dones.object.avto_register_sign is not empty %}{{dones.object.avto_register_sign|trim}}{%endif%}">

    </div>




</div>